<table class="table table-condensed">
  <thead>
    <tr>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
      <td href="javascript:void(0)" onclick="$.post('<%= escape_javascript(fund_transactions_url(@fund, :user_id => user.id, :amount => @fund.single_amount)) %>', function(data) {
          $('.result').html(data);
      });;", data-fund="<%= @fund.id %>", data-user="<%= user.id %>", :data-amount="<%= @fund.single_amount %>" class='add-friend' %><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

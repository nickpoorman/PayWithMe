<h1>Transaction</h1>

<p>
  <b> type</b><br>
  <%= @transaction._type %>
</p>

<p>
  <b></b><br>
  <%= @transaction._id %>
</p>

<p>
  <b>Paid</b><br>
  <%= @transaction.paid %>
</p>

<p>
  <b>Fund</b><br>
  <%= @transaction.fund_id %>
</p>

